{
  "name": "paystation",
  "version": "1.0.2",
  "description": "TypeScript SDK for PayStation's Hosted Checkout Integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run type-check && npm test && npm run build",
    "version": "echo 'Version updated to' $(node -p \"require('./package.json').version\")",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "paystation",
    "payment",
    "typescript",
    "sdk",
    "checkout"
  ],
  "author": "Al-Amin Islam Nerob <alamin@aincoder.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ain477/paystation.git"
  },
  "bugs": {
    "url": "https://github.com/ain477/paystation/issues"
  },
  "homepage": "https://github.com/ain477/paystation#readme",
  "dependencies": {
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.0.1",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
